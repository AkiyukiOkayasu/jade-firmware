{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "${command:cmake.launchTargetPath}",
            "name": "RP2040 Debug",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            // "serverpath": "/Applications/SEGGER/JLink/JLinkGDBServerCLExe",
            "armToolchainPath": "/usr/local/Cellar/arm-none-eabi-gcc/10.3-2021.07/bin",
            "device": "RP2040_M0_0",
            "interface": "swd",
            "serialNumber": "", // add J-Link serial number if having multiple attached the same time.
            "runToMain": true,
            "svdFile": "${env:PICO_SDK_PATH}/src/rp2040/hardware_regs/rp2040.svd",
        }
    ]
}